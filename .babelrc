{
	"presets": [
		"@babel/preset-env",
		"@babel/preset-react",
		"@babel/preset-typescript",
		"@emotion/babel-preset-css-prop"
	],
	"plugins": [
		"@emotion",
		[
			"babel-plugin-jsx-pragmatic",
			{
				"module": "@emotion/core",
				"export": "jsx",
				"import": "jsx"
			}
		],
		[
			"@babel/plugin-proposal-decorators",
			{
				"legacy": true
			}
		],
		"@babel/plugin-proposal-class-properties"
	]
}
